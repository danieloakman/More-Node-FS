{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "curly": false,
    "semicolon": true,
    "max-line-length": [true, 100],
    "no-var-requires": false,
    "no-unused-variable": true,
    "no-unused-expression": true,
    "no-empty": false
  },
  "rulesDirectory": []
}